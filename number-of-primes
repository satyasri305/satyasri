using namespace std;
int main()
{
	long long int N=1000000;
    long long int prime[1000000];
    prime[0]=prime[1]=0;
    for(int i=2;i<=N;i++)
	prime[i]=1;
    int n;
    cin>>n;
    for(int i=2;i*i<=n;i++)
    {
        if(prime[i])
        {
            for(int j=i*i;j<=n;j+=i)
                prime[j]=0;
      	}
	}
    int c=0;
    for(int i=0;i<=n;i++)
	if(prime[i])
	c++;
    cout<<c<<"\n";
	    return 0;
}
